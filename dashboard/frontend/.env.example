# Frontend Environment Configuration
# Copy to .env and configure

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# App Configuration
VITE_APP_TITLE=Neural Command Center
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_EDGE_VIEW=true
VITE_ENABLE_GOVERNANCE=true
VITE_ENABLE_TRAINING=true

# Update Rates
VITE_WS_RECONNECT_DELAY=3000
VITE_CANVAS_FPS=60

# Development
VITE_DEBUG=false
